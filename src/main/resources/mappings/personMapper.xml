<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="learn.PersonDao">

    <insert id="insert" parameterType="learn.Person">
        insert into person(person_id, person_name, person_email, person_sex) values (#{id}, #{name}, #{email}, #{sex})
    </insert>
    
    <select id="getAll">
    
    </select>
</mapper>